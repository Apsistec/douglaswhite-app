{
  "name": "douglaswhite-app",
  "version": "2.0.2",
  "author": "Douglas White <doug@douglaswhite.app>",
  "homepage": "https://www.douglaswhite.app/",
  "description": "A cross-platform PWA which provides background information, skills, and some past projects which was developed using Angular and Ionic by Douglas White, a web application engineer.",
  "scripts": {
    "build": "ng build",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "jest",
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:web": "npm run build"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "private": true,
